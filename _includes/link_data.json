{
  "exitLinkExcludes": [
    {% assign linkCounter = 0 %}
    {% for linkexcludes in site.data.exitlinkexcludes %}
        {% if slinkexcludes.origin != "" %}
          {% if linkCounter != 0 %}
        , 
          {% endif %}
        {
          "origin": "{{linkexcludes.origin}}"
        {% if slinkexcludes.destination != "" %}
      if linkexcludes.origin
      , "destination": "{{linkexcludes.destination}}"
        {% endif %}
        }
        {% endif %}
        {% assign linkCounter = linkCounter | plus:1 %}
    {% endfor %}
  ]
}
